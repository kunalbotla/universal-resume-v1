{
  "name": "universal-resume",
  "version": "2.3.0",
  "description": "Minimal and formal résumé (CV) website template for print, mobile, and desktop. https://bit.ly/2kEzgt8",
  "author": "WebPraktikos <hi@webpraktikos.com>",
  "scripts": {
    "serve": "concurrently \"postcss ./tailwind.css -o docs/styles.css --watch\"  \"live-server ./docs \"",
    "dev": "cross-env NODE_ENV=dev postcss ./tailwind.css -o docs/styles.css",
    "build": "cross-env NODE_ENV=build postcss ./tailwind.css -o docs/styles.css"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.6.4",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.2"
  }
}
